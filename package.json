{
  "name": "@voomsway/react-table",
  "version": "1.0.0-beta.74",
  "description": "React table for voomsway web application",
  "author": "Cmion",
  "license": "MIT",
  "repository": "https://github.com/ekaruztech/react-table.git",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "esmodule": "dist/react-table.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --css-modules false --compress  --format es,cjs,umd",
    "start": "microbundle-crl watch --no-compress --css-modules false --format es,cjs,umd",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "release": "semantic-release --no-ci"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/invariant": "^2.2.34",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.20.6",
    "@types/react": "^17.0.3",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "cross-env": "^7.0.3",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-standard": "^4.1.0",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "semantic-release": "^17.4.2",
    "typescript": "^4.2.3"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "antd": "^4.15.0",
    "date-fns": "2.19.0",
    "framer-motion": "2.9.5",
    "invariant": "2.2.4",
    "lodash": "4.17.21",
    "node-sass": "4.14.1",
    "react-beautiful-dnd": "13.1.0",
    "react-perfect-scrollbar": "1.5.8",
    "remixicon": "2.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}

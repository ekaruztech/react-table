{
  "name": "@voomsway/react-table",
  "version": "1.0.0-beta.22",
  "description": "React table for voomsway web application",
  "author": "Cmion",
  "license": "Apache-2.0",
  "repository": "https://github.com/ekaruztech/react-table.git",
  "main": "dist/index.js",
  "esmodule": "dist/react-table.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --css-modules false --compress  --format es,cjs,umd",
    "start": "microbundle-crl watch --no-compress --css-modules false --format es,cjs,umd",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "release": "semantic-release --no-ci"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/invariant": "^2.2.34",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.161",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-beautiful-dnd": "^13.0.0",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "semantic-release": "^17.1.2",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@ant-design/icons": "^4.2.2",
    "antd": "^4.6.4",
    "framer-motion": "^2.6.13",
    "invariant": "^2.2.4",
    "lodash": "^4.17.20",
    "moment": "^2.28.0",
    "node-sass": "^4.14.1",
    "react-beautiful-dnd": "^13.0.0",
    "react-perfect-scrollbar": "^1.5.8",
    "remixicon": "^2.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
